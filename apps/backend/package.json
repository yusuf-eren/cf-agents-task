{
  "name": "backend",
  "version": "1.0.0",
  "description": "Cloudflare Agents Backend",
  "type": "module",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc --noEmit",
    "test": "vitest",
    "types": "wrangler types",
    "format": "prettier --write .",
