    "@types/node": "^24.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.28.1"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@cf-agents/shared": "*",
    "agents": "^0.0.111",
    "ai": "^4.3.19",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
