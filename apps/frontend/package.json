{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Cloudflare Agents Frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:staging": "VITE_PUBLIC_BACKEND_URL=https://cf-agents-backend.erenyusuf170.workers.dev VITE_PUBLIC_APP_URL=https://cf-agents-frontend-staging.pages.dev vite build",
    "build:production": "VITE_PUBLIC_BACKEND_URL=https://cf-agents-backend.erenyusuf170.workers.dev VITE_PUBLIC_APP_URL=https://cf-agents-frontend.pages.dev vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "deploy:staging": "npm run build:staging && wrangler deploy --env staging",
    "deploy:production": "npm run build:production && wrangler deploy --env production",
    "deploy:prod": "npm run deploy:production",
    "lint": "echo 'Linting disabled'",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.12",
    "@cf-agents/shared": "*",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",

    "agents": "^0.0.111",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "marked": "^16.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1"
  }
}
